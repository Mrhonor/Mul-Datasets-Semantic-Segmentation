      "with_consistence": false,
      "consistent_loss_weight": 10,
      "with_mulbn": false
    },
    "res_save_pth": "res/Mds/",
    "network": {
      "stride": 4,
      "with_domain_adversarial": false
    },
    "dataset_aux_head": {
      "use_dataset_aux_head": false,
      "aux_iter": 15000
    },
    "optim": "AdamW",
    "iter": 0
