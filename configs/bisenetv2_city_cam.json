    "n_datasets": 2,
    "num_unify_classes": 19,
    "hrnet": {
      "cfg": "hrnet48",
      "keep_imagenet_head": false,
      "drop_stage4": false,
      "full_res_stem": false
    },
    "dataset1": {
      "n_cats": 18,
      "data_reader": "CityScapes",
      "im_root": "/data2/datasets",
