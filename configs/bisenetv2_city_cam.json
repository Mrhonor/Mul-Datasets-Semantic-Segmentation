      "type": "MultiLabelCrossEntropyLoss",
      "reweight": false,
      "with_aux": false,
      "aux_weight": 1,
      "aux_num": 4,
      "ignore_index": -1,
      "domain_loss_weight": 1,
      "m": 0,
      "gamma": 12
    },
    "lr": {
      "lr_start": 5e-3,
      "weight_decay": 5e-5,
      "warmup_iters": 0,
      "fix_param_iters": 0,
      "max_iter": 50000
    },
    "train": {
      "scales": [0.5, 1.0, 2.0],
      "cropsize": [512, 512],
      "finetune": false,
      "finetune_from": "res/Mds/model_5000.pth"
    },
    "val": {
      "scales": [0.5, 0.75, 1.0, 1.25, 1.5, 1.75],
      "cropsize": [512, 512]
    },
    "use_fp16": false,
    "use_sync_bn": false,
